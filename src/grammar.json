{
  "name": "modelscript",
  "rules": {
    "module": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "expression",
              "content": {
                "type": "SYMBOL",
                "name": "_expression"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_literal"
        },
        {
          "type": "SYMBOL",
          "name": "array_constructor"
        },
        {
          "type": "SYMBOL",
          "name": "object_constructor"
        },
        {
          "type": "SYMBOL",
          "name": "unary_expression"
        }
      ]
    },
    "_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_number_literal"
        },
        {
          "type": "SYMBOL",
          "name": "_string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "logical_literal"
        },
        {
          "type": "SYMBOL",
          "name": "null_literal"
        }
      ]
    },
    "_number_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_integer_literal"
        }
      ]
    },
    "_integer_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "binary_integer_literal"
        },
        {
          "type": "SYMBOL",
          "name": "octal_integer_literal"
        },
        {
          "type": "SYMBOL",
          "name": "decimal_integer_literal"
        },
        {
          "type": "SYMBOL",
          "name": "hexadecimal_integer_literal"
        }
      ]
    },
    "binary_integer_literal": {
      "type": "PATTERN",
      "value": "0b[0-1]+"
    },
    "octal_integer_literal": {
      "type": "PATTERN",
      "value": "0o[0-7]+"
    },
    "decimal_integer_literal": {
      "type": "PATTERN",
      "value": "[1-9][0-9]*"
    },
    "hexadecimal_integer_literal": {
      "type": "PATTERN",
      "value": "0x[0-9A-Fa-f]+"
    },
    "_string_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "double_quoted_string_literal"
        },
        {
          "type": "SYMBOL",
          "name": "single_quoted_string_literal"
        }
      ]
    },
    "double_quoted_string_literal": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "PATTERN",
            "value": "[^\"]*"
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "single_quoted_string_literal": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "'"
          },
          {
            "type": "PATTERN",
            "value": "[^']*"
          },
          {
            "type": "STRING",
            "value": "'"
          }
        ]
      }
    },
    "logical_literal": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "false"
          },
          {
            "type": "STRING",
            "value": "true"
          }
        ]
      }
    },
    "null_literal": {
      "type": "STRING",
      "value": "null"
    },
    "array_constructor": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "element",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_element"
                  }
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "FIELD",
                        "name": "element",
                        "content": {
                          "type": "SYMBOL",
                          "name": "_element"
                        }
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "object_constructor": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "element",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_element"
                  }
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "FIELD",
                        "name": "element",
                        "content": {
                          "type": "SYMBOL",
                          "name": "_element"
                        }
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyed_element"
        },
        {
          "type": "SYMBOL",
          "name": "unkeyed_element"
        }
      ]
    },
    "keyed_element": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "_expression"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_expression"
          }
        }
      ]
    },
    "unkeyed_element": {
      "type": "FIELD",
      "name": "value",
      "content": {
        "type": "SYMBOL",
        "name": "_expression"
      }
    },
    "unary_expression": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "operator",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "+"
                },
                {
                  "type": "STRING",
                  "value": "-"
                },
                {
                  "type": "STRING",
                  "value": "~"
                },
                {
                  "type": "STRING",
                  "value": "!"
                }
              ]
            }
          },
          {
            "type": "FIELD",
            "name": "operand",
            "content": {
              "type": "SYMBOL",
              "name": "_expression"
            }
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

